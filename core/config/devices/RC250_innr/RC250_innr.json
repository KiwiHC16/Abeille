{
    "RC250_innr": {
        "manufacturer": "Innr",
        "model": "RC250",
        "type": "Innr remote control",
        "timeout": "60",
        "category": {
            "automatism": "1"
        },
        "configuration": {
            "icon": "Innr-Remote-5keys",
            "mainEP": "01",
            "batteryType": "1x3V CR2450"
        },
        "commands": {
            "SWBuildID": {
                "use": "inf_zbAttr-0000-SWBuildID"
            },
            "Get SWBuildID": {
                "use": "zbReadAttribute",
                "params": "clustId=0000&attrId=4000"
            },
            "Battery-Percent": {
                "use": "inf_zbAttr-0001-BatteryPercent"
            },
            "SetReporting 0001-00021": {
                "use": "zbConfigureReporting",
                "params": "clustId=0001&attrType=20&attrId=0021&minInterval=0708&maxInterval=0E10&changeVal=",
