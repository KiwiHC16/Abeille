{
    "KEPZB-110_frientAS": {
        "manufacturer": "Frient",
        "model": "KEYZB-110",
        "type": "Frient keypad",
        "genericType": "Security",
        "timeout": "60",
        "category": {
            "security": "1"
        },
        "configuration": {
            "icon": "Frient-Keypad",
            "mainEP": "2C",
            "batteryType": "4x1.5V AA"
        },
        "commands": {
            "Battery-Volt": {
                "use": "inf_zbAttr-0001-BatteryVolt",
                "params": "ep=2C"
            },
            "SetReporting 2C-0001-00020": {
                "use": "act_zbConfigureReporting",
                "params": "ep=2C&clustId=0001&attrType=20&attrId=0020&minInterval=0708&maxInterval=0E10&changeVal=",
                "execAtCreation": "yes"
            },
            "Bind 2C-0001-ToZigate": {
                "use": "act_zbBindToZigate",
                "params": "ep=2C&clustId=0001",
                "execAtCreation": "yes"
            },
            "Identify": {
                "use": "Identify"
            },

            "Arm Value": {
                "use": "inf_zbCmdC-0501-Arm-Value",
                "isVisible": 1
            },
            "Arm String": {
                "use": "inf_zbCmdC-0501-Arm-String",
                "isVisible": 1,
                "nextLine": "after"
            },
            "ZoneStatus Change": {
                "use": "inf_zbCmdS-0500-ZoneStatus-ChangeNotification",
                "comment": "On receive we trig 2C-SOS with extracted boolean value",
                "trigOut": "2C-SOS",
                "trigOutOffset": "#value#&4"
            },
            "SOS": {
                "use": "inf_generic-Badge",
                "logicalId": "2C-SOS",
                "isVisible": 1
            }
        }
    }
}