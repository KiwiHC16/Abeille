    "zigbeeShade": {
        "timeout": "60",
        "configuration": {
            "mainEP": "01",
            "icon": "zigbeeWindowCoveringDevice"
        },
        "category": {
            "automatism": "1"
        },
        "type": "zigbeeShade",
        "commands": {
            "ZCL Version": {
                "use": "inf_zbAttr-0000-ZCLVersion"
            },
            "Up": {
                "use": "act_zbCmdG-0102-UpOpen"
            },
            "Down": {
                "use": "act_zbCmdG-0102-DownClose"
            },
            "Stop": {
                "use": "act_zbCmdG-0102-Stop"
            },
            "Groups": {
                "use": "Group-Membership",
                "isVisible": 1
            },
            "SWBuildID": {
                "use": "inf_zbAttr-0000-SWBuildID"
            },
            "Get-SWBuildID": {
                "use": "act_zbReadAttribute",
                "params": "clustId=0000&attrId=4000"
            }
