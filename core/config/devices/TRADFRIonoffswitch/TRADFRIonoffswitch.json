{
    "TRADFRIonoffswitch": {
        "manufacturer": "IKEA",
        "zbManufacturer": "IKEAofSweden",
        "model": "E1743",
        "type": "TRADFRI on/off switch",
        "genericType": "Light",
        "comment": "Ref. FR 704.085.95",
        "comment2": "below 2.3.075: suppports binding to groups only, 2.3.075 and greater: supports binding to DEVICES ONLY",
        "timeout": "60",
        "configuration": {
            "remote": {
                "type": "bindToDev"
            },
            "mainEP": "01",
            "icon": "Ikea-OnOffSwitch",
            "batteryType": "1x3V CR2032"
        },
        "category": {
            "light": "1"
        },
        "commands": {
            "SWBuild": {
                "use": "inf_zbAttr-0000-SWBuildID"
            },
            "Get-SWBuild": {
                "use": "act_zbReadAttribute",
                "params": "clustId=0000&attrId=4000",
                "execAtCreation": "Yes"
            },
            "Battery-Percent": {
                "use": "inf_zbAttr-0001-BatteryPercent",
                "calculValueOffset": "#value#*2",
                "comment": "Battery percent reporting seems half the expected value"
            },
            "Bind-0001-ToZigate": {
                "use": "act_zbBindToZigate",
                "params": "clustId=0001",
                "execAtCreation": "Yes"
            },
            "SetReporting-0001-0021": {
                "use": "act_zbConfigureReporting2",
                "params": "clustId=0001&attrType=20&attrId=0021&minInterval=1800&maxInterval=3300",
                "comment": "Reporting every 30 to 55mins max",
                "execAtCreation": "Yes"
            },
            "On": {
                "use": "inf_zbCmdC-0006-On",
                "isVisible": 1
            },
            "Off": {
                "use": "inf_zbCmdC-0006-Off",
                "isVisible": 1
            }
        }
    }
}