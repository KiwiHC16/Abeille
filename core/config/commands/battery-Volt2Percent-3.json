{
    "battery-Volt2Percent-3": {
        "comment": "Computing battery percent from received voltage, assuming max=3V",
        "type": "info",
        "subType": "numeric",
        "configuration": {
            "trigOut": {
                "0001-#EP#-0021" : {
                    "valueOffset": "#value#*100/3"
                }
            }
        },
        "logicalId": "0001-#EP#-0020",
        "unit": "V"
    }
}