{
    "WaterSensor-EM_HEIMAN": {
        "manufacturer": "Heiman",
        "model": "HS1WL",
        "type": "Heiman water leakage sensor",
        "alternateIds": {
            "WaterSensor-N-3.0_HEIMAN": {}
        },
        "timeout": "60",
        "category": {
            "security": "1"
        },
        "configuration": {
            "icon": "Heiman-WaterSensor",
            "mainEP": "01"
        },
        "commands": {
            "Battery-Percent": {
                "use": "zb-0001-BatteryPercent",
                "params": "ep=01"
            },
            "SetReporting 01-0001-00021": {
                "use": "zbConfigureReporting",
                "params": "ep=01&clustId=0001&attrType=20&attrId=0021&minInterval=0708&maxInterval=0E10&changeVal=",
                "execAtCreation": "yes"
            },
            "Bind 01-0001-ToZigate": {
                "use": "zbBindToZigate",
                "params": "ep=01&clustId=0001",
                "execAtCreation": "yes"
            },
            "Identify": {
                "use": "Identify"
            },
            "Zone Alarm1": {
                "use": "attr-Zone-Alarm1",
                "isVisible": "1"
            },
            "Zone Status Changed": {
                "use": "zb-0500-ZoneStatus-ChangeNotification",
                "comment": "On receive we trig <EP>-0500-alarm1 with extracted boolean\/bit0 value",
                "trigOut": "01-0500-alarm1",
                "trigOutOffset": "#value#&1"
            },
            "Bind 0500-ToZigate": {
                "use": "zbBindToZigate",
                "params": "clustId=0500",
                "execAtCreation": "yes"
            }
        }
    }
}