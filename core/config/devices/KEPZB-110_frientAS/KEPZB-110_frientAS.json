{
    "KEPZB-110_frientAS": {
        "manufacturer": "Frient",
        "model": "KEYZB-110",
        "type": "Frient keypad",
        "timeout": "60",
        "category": {
            "security": "1"
        },
        "configuration": {
            "icon": "Frient-Keypad",
            "mainEP": "2C",
            "batteryType": "4x1.5V AA"
        },
        "commands": {
            "Battery-Volt": {
                "use": "inf_zbAttr-0001-BatteryVolt",
                "params": "ep=2C"
            },
            "SetReporting 2C-0001-00020": {
                "use": "act_zbConfigureReporting",
                "params": "ep=2C&clustId=0001&attrType=20&attrId=0020&minInterval=0708&maxInterval=0E10&changeVal=",
                "execAtCreation": "yes"
            },
            "Bind 2C-0001-ToZigate": {
                "use": "act_zbBindToZigate",
                "params": "ep=2C&clustId=0001",
                "execAtCreation": "yes"
            },
            "Identify": {
                "use": "Identify"
            },
            "Zone Alarm1": {
                "use": "attr-Zone-Alarm1",
                "params": "ep=2C",
                "isVisible": "1"
            },
            "Zone Status Changed": {
                "use": "inf_zbAttr-0500-ZoneStatus-ChangeNotification",
                "params": "ep=2C",
                "comment": "On receive we trig <EP>-0500-alarm1 with extracted boolean\/bit0 value",
                "trigOut": "2C-0500-alarm1",
                "trigOutOffset": "#value#&1"
            },
            "Bind 2C-0500-ToZigate": {
                "use": "act_zbBindToZigate",
                "params": "ep=2C&clustId=0500",
                "execAtCreation": "yes"
            }
        }
    }
}